{
	"compilerOptions": {
		"target": "es5",
		"module": "amd",
		"noImplicitAny": true,
		"noEmitHelpers": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"removeComments": true,
		"sourceMap": false,
		"isolatedModules": false,
		"moduleResolution": "classic",
		"rootDir":".",
		"typeRoots": ["./system", "./node_modules"]
	},
	"compileOnSave": true
}
